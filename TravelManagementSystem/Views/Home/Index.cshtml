 <title>AdminHub</title>

			<ul class="box-info">
				<li>
					<i class='bx bxs-calendar-check'></i>
					<span class="text">
					 <!-- Display customer count here -->
			<h3>@ViewBag.CustomerCount</h3>
						<p>New Order</p>
					</span>
				</li>
				<li>
					<i class='bx bxs-group'></i>
					<span class="text">
					<h3>@ViewBag.AgentCount</h3>
